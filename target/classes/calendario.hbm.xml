<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/HibernateMappingDTD5.3//EN"
	"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="proyectoDogSchool.Calendario" table="calendario"
		catalog="proyecto_dog_school">
		<id name="id" type="java.lang.Integer">
			<column name="id" unique="true" />
			<generator class="increment" />
		</id>
		<property name="deporte"></property>
		<property name="fecha"></property>
		<property name="maniana_tarde"></property>
		<property name="infoDia"></property>
		<property name="usuario"></property>
	</class>
</hibernate-mapping>